platform :ios, '10.0'

use_frameworks!

target 'DaisyNet' do

pod 'Alamofire', '>= 4.5.1'
pod 'Cache', '>= 5.0.0'

end

post_install do |installer|
    installer.pods_project.targets.each do |target|
        target.build_configurations.each do |config|
            config.build_settings['IPHONEOS_DEPLOYMENT_TARGET'] = '10.0'
        end
    end
end

